   "source": [
    "import math\n",
    "N, K = map(int, input().split())\n",
    "\n",
    "def func(dice):\n",
    "    if dice >= K:\n",
    "        return 1.0\n",
    "    return (1 / 2) ** math.ceil(math.log2(K / dice))\n",
    "\n",
    "l = [func(dice) / N for dice in range(1, N + 1)]\n",
    "print(sum(l))"
   ]
