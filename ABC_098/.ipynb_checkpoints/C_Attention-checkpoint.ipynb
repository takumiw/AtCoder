    "N = int(input())\n",
    "S = input()\n",
    "e_count = S.count('E')\n",
    "w_count = 0\n",
    "ans = e_count\n",
    "for s in S:\n",
    "    if s == 'E':\n",
    "        e_count -= 1\n",
    "    else:\n",
    "        w_count += 1\n",
    "    if e_count + w_count < ans:\n",
    "        ans = e_count + w_count\n",
    "print(ans)"
