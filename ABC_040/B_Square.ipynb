    "import math\n",
    "n = int(input())\n",
    "min_value = n\n",
    "\n",
    "for i in range(1, int(math.sqrt(n)) + 1):\n",
    "    temp_value = abs(n // i - i) + n - i * (n // i)\n",
    "    if temp_value < min_value:\n",
    "        min_value = temp_value\n",
    "        \n",
    "print(min_value)"
   ]
