   "source": [
    "S = list(map(int, list(input())))\n",
    "colors = [0, 1]\n",
    "cnt_b, cnt_w = 0, 0\n",
    "b_list = [colors[i % 2] for i in range(len(S))]\n",
    "\n",
    "for i in range(len(S)):\n",
    "    if S[i] == b_list[i]:\n",
    "        cnt_w += 1\n",
    "    else:\n",
    "        cnt_b += 1\n",
    "        \n",
    "print(min([cnt_w, cnt_b]))"
   ]
